// 14.2 swap using func (call by reference)
//shehzad khan
#include <stdio.h>

void main()
{
 int a =6;
 int b =9;
 printf("before swap value of a=%d and b=%d\n",a,b);
 swap(&a,&b);
 printf("after swap value of a=%d and b=%d",a,b);
}
    void swap (int *x, int *y)
    {
        int temp;
        temp=*x;
        *x=*y;
        *y=temp;
    }
