#include <stdio.h>
#include <string.h>

void main()
{
    char a[20];
    char b[20];
    printf("enter  vlaue of a:");
    scanf("%s",&a);
    printf("enter  value of b:");
    scanf("%s",&b);
    strcat(a,b);
    printf("a:%s\n",a);
    printf("b:%s\n",b);
    
}
    
