#include <stdio.h>
void main()
{
    int a=4;
    int b=7;
    swap(a,b);
}
void swap (int x,int y)
    {
    int temp;
    temp=x;
    x=y;
    y=temp;
    printf("after sawpping value of x=%d",x);
    printf("\nafter swapping value of y=%d",y);
    }

