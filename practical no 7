//date 01/10/2022
//practical no 7
void main() {
    int week;
    
    printf("enter week number 1 to 7= ");
    scanf ("%d",&week);
     
     switch(week)
       
    {
    case 1:
    printf("monday");
    break;
    case 2:
    printf("tuesday");
    break;
    case 3:
    printf("wednesday");
    break;
    case 4:
    printf("thursday");
    break;
    case 5:
    printf("friday");
    break;
    case 6:
    printf("saturday");
    break;
    case 7:
    printf("sunday");
    break;
     
     default:
     printf("invalid input please enter week number 1-7.");
    }
}
